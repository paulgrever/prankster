<div class="container pads">
  <h1 class="text-center">Pranksters</h1>
  <div class="row">
    <div class="col-lg-6 col-md-6 col-sm-12 col-lg-offset-3 text-center">
      <%= form_for :session, url: '/login' do |f| %>
        <div class="form-group">
          <%=f.label "Existing Users Login", for:"exampleInputEmail1"  %>
          <%= f.text_field :email, class:"form-control", id: "exampleInputEmail1", placeholder:"Email" %>
        </div>
        <div class="form-group">
          <%= f.password_field  :password, class:"form-control", id: "exampleInputPassword1", placeholder:"Password" %>
        </div>
    
        <%= f.submit 'Sign in', class: "btn btn-primary", id: "sign-in"%>
      <% end %>
    </div>
  </div>
  <div class="row">
    <div class="col-lg-6 col-md-6 col-sm-12 col-lg-offset-3 text-center">
      <%= link_to("Sign Up", "#") %>
    </div>
  </div>
</div>